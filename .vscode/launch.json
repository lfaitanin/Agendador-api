{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nest.js Debug",
      "runtimeArgs": ["run-script", "start:dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
